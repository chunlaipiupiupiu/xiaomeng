.container {
  width: 100%;
  display: flex;
  flex-direction: column;
  background-color: #fff;
}

/* header开始 */

.header {
  text-align: center;
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.header image {
  width: 100%;
  height: 370rpx;
}

.joinus {
  justify-content: center;
  align-items: center;
}

/*字体*/

@font-face {
  font-family: 'Sigmar One';
  src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
         url(data:application/font-woff;charset=utf-8;base64,) format('woff');
  font-weight: normal;
  font-style: normal;
}

#site-title {
  animation: shake-it 1.5s reverse infinite cubic-bezier(0.68, -0.55, 0.27, 1.55);
  font-family: Sigmar One !important;
  font-size: 60rpx !important;
  color: #fff !important;
  text-align: center;
}

/*文字抖动*/

@keyframes shake-it {
  0% {
    text-shadow: 0 0 rgba(0, 255, 255, 0.5), 0 0 rgba(255, 0, 0, 0.5);
  }

  25% {
    text-shadow: -1px 0 rgba(0, 255, 255, 0.5), 1px 0 rgba(255, 0, 0, 0.5);
  }

  50% {
    text-shadow: -3px 0 rgba(0, 255, 255, 0.5), 2px 0 rgba(255, 0, 0, 0.5);
  }

  100% {
    text-shadow: 2px 0 rgba(0, 255, 255, 0.5), 3px 0 rgba(255, 0, 0, 0.5);
  }
}

/* header结束 */

.department {
  font-size: 24px;
  line-height: 26px;
}

.officer {
  margin-top: 20px;
}

.join {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.title {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.title view {
  display: flex;
  justify-content: center;
  align-items: center;
}

.title text {
  font-size: 20px;
  margin: 0 20rpx;
  color: rgb(29, 19, 19);
}

.line {
  width: 80rpx;
  height: 1px;
  background: #6cf;
}

.content {
  width: 80%;
  margin: 0 auto;
  font-size: 16px !important;
  text-indent: 20rpx;
  text-align: left;
  word-spacing: 5rpx;
  margin-bottom: 30rpx;
  margin-top: 30rpx;
}

.content text {
  font-size: 14px;
}

.word-color {
  color: #6cf;
  font-size: 16px !important;
}

/* 表单组件开始 */

form {
  width: 100%;
  padding-top: 10px;
  display: flex;
  flex-direction: column;
  justify-content: center;
  background-color: rgb(245, 245, 245);
}

.section {
  width: 100%;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 16px;
  background-color: #fff;
  border-bottom: 1px solid rgb(245, 245, 245);
}

.section-text {
  position: relative;
  width: 100%;
  /* height: 80rpx; */
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 16px;
  background-color: #fff;
  border-bottom: 1px solid rgb(245, 245, 245);
  /* overflow: hidden; */
}

.form-title, input, .picker {
  padding-left: 30rpx;
}

.picker-text {
  padding-left: 44rpx;
}

.section label {
  width: 120rpx;
  height: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.picker-text {
  font-size: 16px;
}

.form-title {
  height: 80rpx;
  font-size: 16px;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  border-bottom: 1px solid rgb(245, 245, 245);
}

textarea, .txtarea {
  position: relative;
  top: 0;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: flex-start;
  background: #fff;
  padding-left: 30rpx;
  height: 200rpx;
}

.signup {
  width: 400rpx;
  margin: 40rpx auto;
  padding: 0;
  background-color: #6cf;
  color: #fff;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.tips text {
  font-size: 12px;
  color: #6cf;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  flex-wrap: wrap;
}
